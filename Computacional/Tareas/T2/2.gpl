set terminal png 
set output "2.png"
set zeroaxis lt -1 lc -1
set xlabel 'Distancia del electrón al núcleo r  [Angstrom]'
set grid xtics
set grid ytics
set mytics 4
set mxtics 4

ao = 0.529167
a = 0
b = 1.2
f(x) = (4*x**2*exp(-2*x/ao))/(ao**3)
plot [a:b] f(x) t "g(r)" w lp pt 12 lw 2 ps 0.6 lc 15

