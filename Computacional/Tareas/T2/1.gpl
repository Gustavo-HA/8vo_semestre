set terminal epslatex color
set output "1.tex"
set zeroaxis lt -1 lc -1
set xlabel 'Longitud del enlace $r$ [m]'
set key at 3.2,30.0
set grid xtics
set grid ytics
set mytics 4
set mxtics 4
k = 14.4
vo = 1.09e3
ro = 0.33
a = 1
b = 3.5
f(x) = -k/x + vo*exp(-x/ro)

plot [a:b] f(x) t "$V(r)$" w l lw 3

#FORMATO KEY, X, V(R)

